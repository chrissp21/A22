<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="305.0" prefWidth="421.0" spacing="20.0" xmlns="http://javafx.com/javafx/18"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="a22.climoilou.sim203.formatif.formatif5.PrimaryController">
    <children>
        <VBox alignment="CENTER">
            <children>
                <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" progress="0.0"/>
                <Label fx:id="messageFichierLabel" text="Progrès fichiers"/>
                <HBox alignment="CENTER" prefHeight="49.0" prefWidth="284.0" spacing="10.0">
                    <children>
                        <Label text="Service fichier"/>
                        <Button fx:id="EcritFichierButton" onAction="#ecritFichier" text="écrit">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="litFichierButton" onAction="#litFichier" text="lit">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </Button>
                        <TextField fx:id="fichierTextField"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Button mnemonicParsing="false" onAction="#annule" text="Anuller"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
